    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "echarts": "^6.0.0",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "mockjs": "^1.1.0",
    "monaco-editor": "^0.52.2",
