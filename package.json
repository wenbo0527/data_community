{
  "name": "data-community-platform",
  "version": "1.0.0",
  "description": "数据社区平台",
  "scripts": {
    "dev": "node scripts/dev-with-subapps.js",
    "dev:main": "vite",
    "dev:all": "node scripts/dev-with-subapps.js",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.vue,.ts,.tsx --fix",
    "lint:check": "eslint src --ext .js,.vue,.ts,.tsx",
    "lint:migrate": "eslint src/pages/risk src/router/model-offline-analysis.js src/store/modules/model-offline.js src/api/offlineModel --ext .js,.vue,.ts,.tsx",
    "typecheck": "vue-tsc -p tsconfig.typecheck.json --noEmit",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:performance": "node scripts/performance-test.js",
    "test:responsive": "node scripts/responsive-test.js",
    "migration:check": "node scripts/migration-check.js",
    "migration:validate": "npm run lint:check && npm run typecheck && npm run migration:check",
    "audit": "npm audit --audit-level high",
    "depcheck": "depcheck",
    "clean": "rimraf dist node_modules/.cache"
  },
  "dependencies": {
    "@antv/x6": "^1.35.0",
    "@antv/x6-vue-shape": "^3.0.2",
    "@arco-design/web-vue": "^2.55.0",
    "@dagrejs/dagre": "^1.1.8",
    "@monaco-editor/loader": "^1.7.0",
    "@vueup/vue-quill": "^1.2.0",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.7",
    "core-js": "^3.48.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.562.0",
    "marked": "^17.0.1",
    "pinia": "^2.1.7",
    "regenerator-runtime": "^0.14.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "depcheck": "^1.4.7",
    "esbuild": "^0.27.2",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "jscpd": "^3.5.10",
    "jsdom": "^24.0.0",
    "less": "^4.5.1",
    "mockjs": "^1.1.0",
    "rimraf": "^5.0.5",
    "sass": "^1.97.3",
    "terser": "^5.46.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-mock": "^3.0.2",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.4"
}
