{
  "name": "data_comunity",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .vue,.js,.ts --cache",
    "lint:fix": "eslint src --ext .vue,.js,.ts --fix --cache",
    "lint:fast": "eslint src --ext .vue,.js,.ts --cache --max-warnings 0",
    "type-check": "vue-tsc --noEmit --incremental",
    "type-check:fast": "vue-tsc --noEmit --incremental --skipLibCheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "quality:check": "npm run lint && npm run type-check",
    "quality:check:fast": "npm run lint:fast && npm run type-check:fast",
    "quality:check:parallel": "concurrently \"npm run lint:fast\" \"npm run type-check:fast\"",
    "quality:fix": "npm run lint:fix && npm run type-check",
    "cache:clean": "rm -rf .eslintcache .tsbuildinfo node_modules/.cache",
    "check": "npm run quality:check:parallel"
  },
